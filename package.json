{
  "name": "react-autocomplete",
  "version": "1.8.2",
  "description": "Accessible, extensible, Autocomplete for React.js",
  "main": "lib/Autocomplete.js",
  "files": [
    "lib/index.js",
    "lib/Autocomplete.js",
    "lib/utils.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/reactjs/react-autocomplete.git"
  },
  "homepage": "https://github.com/reactjs/react-autocomplete",
  "bugs": "https://github.com/reactjs/react-autocomplete/issues",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "clean": "rimraf lib",
    "compile": "babel src --loose --out-dir lib",
    "prepublish": "npm run prepare",
    "prepare": "npm run clean && npm run compile",
    "test": "mocha test/setup.js test/spec/*.js"
  },
  "authors": [
    "Ryan Florence <rpflorence@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.5",
    "enzyme": "^2.8.2",
    "eslint": "^2.13.1",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^6.10.3",
    "exorcist": "^0.4.0",
    "jest": "^20.0.4",
    "lodash.flow": "^3.5.0",
    "lodash.map": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "mocha": "^5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.5.2",
    "st": "^1.2.0",
    "uglify-js": "^2.7.5",
    "watchify": "^3.8.0",
    "youemdee": "^1.0.0"
  },
  "tags": [
    "react",
    "autocomplete",
    "combobox",
    "a11y"
  ],
  "keywords": [],
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "dom-scroll-into-view": "1.0.1",
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0-0 || ^16.0.0-0",
    "react-dom": "^0.14.7 || ^15.0.0-0 || ^16.0.0-0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/dist/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/dist/"
    ]
  }
}
